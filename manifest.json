{
  "manifest_version": 3,
  "name": "FB Marketplace Automator",
  "version": "1.0",
  "permissions": ["tabs", "scripting"],
  "host_permissions": [
    "https://www.facebook.com/*"
  ],
  "action": {
    "default_popup": "popup.html",  
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
